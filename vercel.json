{
  "version": 2,
  "buildCommand": "npm install",
  "routes": [
    {
      "src": "/api/cv/pdf",
      "dest": "/api/cv-pdf.js"
    },
    {
      "src": "/api/cv/preview",
      "dest": "/api/cv-preview.js"
    },
    {
      "src": "/api/cv/templates",
      "dest": "/api/cv-templates.js"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/cv-generator",
      "dest": "/frontend/cv-generator.html"
    },
    {
      "src": "/",
      "dest": "/frontend/cv-generator.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
